read_jdbaba <- function(fn) {
tt <- readLines(fn)
idx <- grep("Minimum|Maximum", tt)
p1 <- tt[idx]
p2 <- tt[setdiff(seq_along(tt), idx)]
idx <- diff(c(idx, length(tt)+1))-1
p2.m <- do.call(rbind, strsplit(p2, "[ ]+"))
mode(p2.m) <- "numeric"
p2.m <- as.data.frame(p2.m)
cbind(p2.m, id=rep(p1, idx))
}
