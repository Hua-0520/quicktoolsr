temp <- lapply(seq_along(myMatch), function(y) {
t1 <- matrix(scan(textfile, skip = myMatch[y],
n = scanVals[y]*2), ncol = 2, byrow = TRUE)
colnames(t1) <- c("time", "value")
cbind(data.frame(t1),
name = gsub("^\\s+([0-9]+)\\s+(.*)", "\\2",
x[myMatch])[y])
})
temp
}
